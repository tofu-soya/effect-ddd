{
  "name": "yl-ddd-ts",
  "version": "2.0.33",
  "description": "Node typescript seedwork",
  "main": "dist/src/index.js",
  "author": "tuancaurao <tuancr.pt@gmail.com>",
  "license": "MIT",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/src/*",
        "./dist/src/index.d.ts"
      ]
    }
  },
  "keywords": [
    "ddd",
    "utils"
  ],
  "files": [
    "dist",
    "tsconfig.json",
    "README"
  ],
  "scripts": {
    "build": "rimraf dist && tsc --project tsconfig.build.json && tsc-alias -p tsconfig.json",
    "trace-resolution": "tsc --traceResolution",
    "lint": "eslint . --ext .ts",
    "lint-fix": "eslint . --ext .ts --fix",
    "test": "jest --config jest.config.js",
    "npm-publish": "yarn test && yarn lint && yarn build && yarn publish",
    "only-publish": "yarn build && yarn publish",
    "preversion": "npm run lint",
    "version": "git add -A src",
    "postversion": "git push && git push --tags",
    "typedoc": "typedoc src/index.ts --out ./docs --tsconfig ./tsconfig.build.json"
  },
  "exports": {
    ".": "./dist/src/index.js",
    "./aim": {
      "import": "./dist/src/aim/index.js"
    },
    "./typeorm": {
      "import": "./dist/src/typeorm.js",
      "types": "./dist/src/typeorm.d.ts"
    },
    "./keyvalue": {
      "import": "./dist/src/ports/database/keyvalue/index.js"
    }
  },
  "dependencies": {
    "@fp-ts/optic": "0.19.0",
    "amqp-connection-manager": "^4.1.11",
    "amqplib": "^0.10.3",
    "decimal.js": "^10.4.3",
    "dotenv": "^16.0.2",
    "effect": "^2.0.0-next.60",
    "fp-ts": "^2.16.1",
    "got": "11.8.3",
    "helmet": "^6.0.0",
    "http-status": "^1.5.2",
    "immer": "^10.0.2",
    "io-ts": "^2.2.20",
    "io-ts-types": "^0.5.19",
    "monocle-ts": "^2.3.13",
    "morgan": "^1.10.0",
    "nanoid": "^3.3.4",
    "newtype-ts": "^0.3.5",
    "oxide.ts": "^1.1.0",
    "ramda": "^0.28.0",
    "redis": "^4.6.5",
    "reflect-metadata": "^0.1.13",
    "shallow-equal-object": "^1.1.1",
    "supertokens-node": "^16.4.0",
    "ts-pattern": "^5.0.5",
    "typeorm": "0.3.20",
    "uuid": "^9.0.1",
    "validator": "^13.11.0",
    "winston": "^3.8.1",
    "xss-clean": "^0.1.1"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "devDependencies": {
    "@types/amqplib": "^0.10.1",
    "@types/got": "^9.6.12",
    "@types/jest": "^29.5.4",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.7.14",
    "@types/ramda": "^0.28.23",
    "@types/uuid": "^8.3.4",
    "@types/validator": "^13.11.2",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.4.3",
    "prettier": "3.1.1",
    "redis-mock": "^0.56.3",
    "rimraf": "^4.1.2",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.2",
    "tsconfig-paths": "^4.1.0",
    "type-fest": "^4.21.0",
    "typedoc": "^0.25.13",
    "typescript": "~5.3.3",
    "typescript-transform-paths": "^3.4.6"
  },
  "peerDependencies": {
    "@fp-ts/optic": "^0.20.0"
  },
  "packageManager": "yarn@4.1.1"
}
